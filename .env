STUN_SERVERS="stun:stun.l.google.com:19302,stun:stun1.l.google.com:19302,stun:stun2.l.google.com:19302"

# Fallback STUN servers (if primary STUN servers fail)
FALLBACK_STUN_SERVERS="stun:stun3.l.google.com:19302,stun:stun4.l.google.com:19302"

